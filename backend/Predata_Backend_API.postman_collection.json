					}
				},
				{
					"name": "Get Activities by Member",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/activities/member/{{memberId}}", "host": ["{{baseUrl}}"], "path": ["api", "activities", "member", "{{memberId}}"] }
					}
				},
