					"name": "Get Activities by Member (Vote Only)",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/activities/member/{{memberId}}?type=VOTE", "host": ["{{baseUrl}}"], "path": ["api", "activities", "member", "{{memberId}}"], "query": [{ "key": "type", "value": "VOTE" }] }
					}
				},
				{
					"name": "Get Activities by Question",
					"request": {
						"method": "GET",
