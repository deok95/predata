				},
				{
					"name": "Get Member's Active Orders",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/orders/member/{{memberId}}", "host": ["{{baseUrl}}"], "path": ["api", "orders", "member", "{{memberId}}"] }
					}
				},
				{
					"name": "Get Member's Orders for Question",
