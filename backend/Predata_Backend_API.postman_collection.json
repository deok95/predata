{
	"info": {
		"_postman_id": "predata-backend-api",
		"name": "Predata Backend API",
		"description": "Predata Backend API Collection - Phase 1 Security Fix Applied",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"variable": [
		{ "key": "baseUrl", "value": "http://localhost:8080", "type": "string" },
		{ "key": "memberId", "value": "1", "type": "string" },
		{ "key": "questionId", "value": "1", "type": "string" },
		{ "key": "token", "value": "", "type": "string" }
	],
	"auth": {
		"type": "bearer",
		"bearer": [{ "key": "token", "value": "{{token}}", "type": "string" }]
	},
	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Send Verification Code",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/send-code", "host": ["{{baseUrl}}"], "path": ["api", "auth", "send-code"] },
						"body": { "mode": "raw", "raw": "{\n  \"email\": \"test@example.com\"\n}" }
					}
				},
				{
					"name": "Verify Code",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/verify-code", "host": ["{{baseUrl}}"], "path": ["api", "auth", "verify-code"] },
						"body": { "mode": "raw", "raw": "{\n  \"email\": \"test@example.com\",\n  \"code\": \"123456\"\n}" }
					}
				},
				{
					"name": "Complete Signup",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/complete-signup", "host": ["{{baseUrl}}"], "path": ["api", "auth", "complete-signup"] },
						"body": { "mode": "raw", "raw": "{\n  \"email\": \"test@example.com\",\n  \"code\": \"123456\",\n  \"password\": \"Test1234!\",\n  \"passwordConfirm\": \"Test1234!\"\n}" }
					}
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (jsonData.token) {",
									"    pm.collectionVariables.set('token', jsonData.token);",
									"    console.log('Token saved:', jsonData.token.substring(0, 20) + '...');",
									"}",
									"if (jsonData.memberId) {",
									"    pm.collectionVariables.set('memberId', jsonData.memberId.toString());",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/login", "host": ["{{baseUrl}}"], "path": ["api", "auth", "login"] },
						"body": { "mode": "raw", "raw": "{\n  \"email\": \"admin@predata.io\",\n  \"password\": \"123400\"\n}" }
					}
				},
				{
					"name": "Google OAuth Login",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/google", "host": ["{{baseUrl}}"], "path": ["api", "auth", "google"] },
						"body": { "mode": "raw", "raw": "{\n  \"googleToken\": \"YOUR_GOOGLE_ID_TOKEN\"\n}" }
					}
				},
				{
					"name": "Complete Google Registration",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/auth/google/complete-registration", "host": ["{{baseUrl}}"], "path": ["api", "auth", "google", "complete-registration"] },
						"body": { "mode": "raw", "raw": "{\n  \"googleId\": \"google-sub-id\",\n  \"email\": \"user@gmail.com\",\n  \"countryCode\": \"KR\"\n}" }
					}
				}
			]
		},
		{
			"name": "Members",
			"item": [
				{
					"name": "Create Member (Signup)",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/members", "host": ["{{baseUrl}}"], "path": ["api", "members"] },
						"body": { "mode": "raw", "raw": "{\n  \"email\": \"newuser@example.com\",\n  \"walletAddress\": null,\n  \"countryCode\": \"KR\",\n  \"jobCategory\": \"IT\",\n  \"ageGroup\": 30\n}" }
					}
				},
				{
					"name": "Get Member by ID",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/members/{{memberId}}", "host": ["{{baseUrl}}"], "path": ["api", "members", "{{memberId}}"] }
					}
				},
				{
					"name": "Get Current User (JWT)",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/members/me", "host": ["{{baseUrl}}"], "path": ["api", "members", "me"] }
					}
				},
				{
					"name": "Get Member by Email",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/members/by-email?email=admin@predata.io", "host": ["{{baseUrl}}"], "path": ["api", "members", "by-email"], "query": [{ "key": "email", "value": "admin@predata.io" }] }
					}
				},
				{
					"name": "Get Member by Wallet",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/members/by-wallet?address=0x1234", "host": ["{{baseUrl}}"], "path": ["api", "members", "by-wallet"], "query": [{ "key": "address", "value": "0x1234" }] }
					}
				}
			]
		},
		{
