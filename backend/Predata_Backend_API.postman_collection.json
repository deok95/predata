						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/orders/member/{{memberId}}/question/{{questionId}}", "host": ["{{baseUrl}}"], "path": ["api", "orders", "member", "{{memberId}}", "question", "{{questionId}}"] }
					}
				}
			]
		},
		{
			"name": "Settlement",
			"item": [
				{
					"name": "Initiate Settlement",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/questions/{{questionId}}/settle", "host": ["{{baseUrl}}"], "path": ["api", "questions", "{{questionId}}", "settle"] },
						"body": { "mode": "raw", "raw": "{\n  \"finalResult\": \"YES\",\n  \"sourceUrl\": \"https://example.com/result\"\n}" }
					}
				},
				{
					"name": "Finalize Settlement",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/questions/{{questionId}}/settle/finalize", "host": ["{{baseUrl}}"], "path": ["api", "questions", "{{questionId}}", "settle", "finalize"] },
						"body": { "mode": "raw", "raw": "{\n  \"force\": false\n}" }
					}
				},
				{
					"name": "Cancel Settlement",
					"request": {
						"method": "POST",
						"url": { "raw": "{{baseUrl}}/api/questions/{{questionId}}/settle/cancel", "host": ["{{baseUrl}}"], "path": ["api", "questions", "{{questionId}}", "settle", "cancel"] }
