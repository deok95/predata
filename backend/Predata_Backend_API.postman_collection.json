					}
				}
			]
		},
		{
			"name": "Orders",
			"item": [
				{
					"name": "Get Order Book",
					"request": {
						"method": "GET",
						"url": { "raw": "{{baseUrl}}/api/questions/{{questionId}}/orderbook", "host": ["{{baseUrl}}"], "path": ["api", "questions", "{{questionId}}", "orderbook"] }
					}
				},
				{
					"name": "Create Limit Order",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"url": { "raw": "{{baseUrl}}/api/orders", "host": ["{{baseUrl}}"], "path": ["api", "orders"] },
						"body": { "mode": "raw", "raw": "{\n  \"memberId\": {{memberId}},\n  \"questionId\": {{questionId}},\n  \"side\": \"YES\",\n  \"price\": 60,\n  \"quantity\": 10\n}" }
					}
				},
				{
					"name": "Cancel Order",
					"request": {
						"method": "DELETE",
						"url": { "raw": "{{baseUrl}}/api/orders/1?memberId={{memberId}}", "host": ["{{baseUrl}}"], "path": ["api", "orders", "1"], "query": [{ "key": "memberId", "value": "{{memberId}}" }] }
