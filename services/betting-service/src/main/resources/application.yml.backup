spring:
  application:
    name: betting-service
  datasource:
    url: jdbc:mariadb://localhost:3306/predata_betting
    username: root
    password: 1234
    driver-class-name: org.mariadb.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
  redis:
    host: localhost
    port: 6379

server:
  port: 8083

services:
  member:
    url: http://localhost:8081
  question:
    url: http://localhost:8082

logging:
  level:
    com.predata.betting: DEBUG
